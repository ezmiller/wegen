{
  "name": "wegen",
  "version": "0.1.3",
  "description": "Dead-simple client-side routing with an eye towards Flux architectures",
  "main": "dist/wegen.js",
  "scripts": {
    "build": "webpack && webpack --config webpack.config.min.js",
    "prepublish": "npm test && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pletcher/wegen.git"
  },
  "author": {
    "name": "Charles Pletcher",
    "url": "https://github.com/pletcher"
  },
  "keywords": [
    "flux",
    "router",
    "javascript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pletcher/wegen/issues"
  },
  "dependencies": {
    "immutable": "^3.7.2",
    "react": "^0.13.1"
  },
  "devDependencies": {
    "babel": "^4.1.1",
    "babel-core": "^4.1.1",
    "babel-jest": "^5.0.1",
    "babel-loader": "^4.0.0",
    "jest-cli": "^0.4.0",
    "react-tools": "^0.12.2"
  },
  "jest": {
    "collectCoverage": false,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "immutable",
      "react"
    ]
  }
}
